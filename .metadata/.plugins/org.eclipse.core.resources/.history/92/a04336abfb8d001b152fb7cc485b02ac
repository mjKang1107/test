/*
 
 this() 메소드 -> 같은 클래스의 다른 생성자 호출시 사용
			 
주제 : 같은 클래스 내부의 다른형태의 생성자를 호출하는 this()메소드 알아보기
 
 */
class MyDate{
	private int year;
	private int month;
	private int day;
	
	//객체생성시!! 객체변수 year,month,day의 값을 저장할 생성자
	public MyDate(int year, int month, int day) {
		
	}
	
	//위 객체 변수(인스턴스 변수)의 값들을 출력하는 기능의 메소드 
	public void print() {
		System.out.println(this.year + "/" + this.month + "/" + day);
	}
	
}



public class Test66 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
