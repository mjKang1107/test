
//클래스 설계도
public class Point {
	//변수
	int x,y;
	//기본생성자(매개변수가x)
	Point() {
		System.out.println("Point기본생성자 호출됨");
		x = 2;
		y = 2;
		
	}
	//이름은 같게 매개변수 달리해서 여러개 생성자 생성가능(생성자 오버로딩)
	//x변수의 값을 매개변수로 전달받아 초기화할 생성자
	Point(int n_x) {
		x = n_x;
		
	}
	//x,y 변수의 값을 매개변수로 전달받아 초기화할 생성자
	Point(int n_x , int n_y){
		x = n_x;
		y = n_y;
		
	}
	
	
	
	
}
