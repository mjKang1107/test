//클래스, 객체 생성, 생성자 만들기 연습

//MyDate라는 이름의 클래스 정의

class MyDate{
	//정수값을 저장할 year,month, day 변수 정의
	int year,month, day;
	//기본 생성자 정의
		//year 2014
		//month 1
		//day 1
public MyDate() {
	int year = 2014;
	int month = 1;
	int day = 1;
}	
	
	//년도 값을 n_year매개변수로 전달 받아
		//year = n_year저장
		//month = 1 
		//day = 1
public MyDate() {
	
}		
	//년도 , 월값을 n_year, n_ month매개변수로 전달 받아
		//year n_year
		//month n_month
		//day 1


	//년도 , 월, 일값을 n_year, n_ month, n_day매개변수로 전달 받아
		//year n_year
		//month n_month
		//day n_day	


public MyDate() {
	
}		




}
public class Test63 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
